<include>
	<extension name="evtsockapp">
		<condition field="destination_number" expression="^(.*)$">
			<action application="socket" data="127.0.0.1:8040"/>
		</condition>
		<condition field="destination_number" expression="^(441)$">
			<action application="socket" data="127.0.0.1:8040"/>
		</condition>
		<condition field="destination_number" expression="^(\*\d{9})$">
			<action application="socket" data="127.0.0.1:8040"/>
		</condition>
	</extension>
</include>
